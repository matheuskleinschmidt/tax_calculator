// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["deno"]
  output = "../generated/client"
}

datasource db {
  provider = "postgresql"
  url      = Deno.env.get("DATABASE_URL")
}

model Dinosaur {
  id          Int     @id @default(autoincrement())
  name        String  @unique
  description String
}

model taxRanges {
  id          Int     @id @default(autoincrement())
  name        String  @unique
  min         Int
  max         Int
  aliquot     Int
  AmountToDeduct Int
  IRPJ        Int
  CSLL        Int
  Cofins      Int
  PisPasep    Int
  CPP         Int
  ICMS        Int
  description String
}

